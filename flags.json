{
  "dark-mode": {
    "variations": {
      "enabled": true,
      "disabled": false
    },
    "defaultRule": {
      "variation": "disabled"
    }
  },
  "ai-assistant": {
    "variations": {
      "off": "none",
      "beta": "basic-nlp",
      "advanced": "gpt-powered"
    },
    "defaultRule": {
      "variation": "off"
    }
  },
  "cache-strategy": {
    "variations": {
      "none": "none",
      "memory": "in-memory",
      "redis": "redis",
      "hybrid": "memory+redis"
    },
    "defaultRule": {
      "variation": "memory"
    }
  },
  "upload-max-size": {
    "variations": {
      "small": "10MB",
      "medium": "100MB",
      "large": "1GB"
    },
    "defaultRule": {
      "variation": "medium"
    }
  },
  "ab-test-homepage": {
    "variations": {
      "original": {
        "banner": "classic",
        "cta": "blue"
      },
      "variant-a": {
        "banner": "modern",
        "cta": "green"
      },
      "variant-b": {
        "banner": "minimal",
        "cta": "black"
      }
    },
    "defaultRule": {
      "variation": "original"
    }
  },
  "fraud-detection": {
    "variations": {
      "disabled": {
        "mode": "off"
      },
      "standard": {
        "mode": "ml",
        "sensitivity": 0.5
      },
      "strict": {
        "mode": "ml",
        "sensitivity": 0.8
      }
    },
    "defaultRule": {
      "variation": "standard"
    }
  },
  "backup-strategy": {
    "variations": {
      "daily": "every-24h",
      "hourly": "every-1h",
      "realtime": "continuous"
    },
    "defaultRule": {
      "variation": "daily"
    }
  },
  "password-policy": {
    "variations": {
      "lenient": {
        "minLength": 6,
        "requireSymbols": false
      },
      "standard": {
        "minLength": 8,
        "requireSymbols": true
      },
      "strict": {
        "minLength": 12,
        "requireSymbols": true,
        "require2FA": true
      }
    },
    "defaultRule": {
      "variation": "standard"
    }
  },
  "ads-personalization": {
    "variations": {
      "disabled": false,
      "enabled": true
    },
    "defaultRule": {
      "variation": "disabled"
    }
  },
  "video-quality-default": {
    "variations": {
      "sd": "480p",
      "hd": "1080p",
      "uhd": "4K"
    },
    "defaultRule": {
      "variation": "hd"
    }
  },
  "search-filters": {
    "variations": {
      "basic": [
        "category",
        "price"
      ],
      "extended": [
        "category",
        "price",
        "rating",
        "availability"
      ],
      "full": [
        "category",
        "price",
        "rating",
        "availability",
        "brand",
        "delivery"
      ]
    },
    "defaultRule": {
      "variation": "basic"
    }
  },
  "voice-recognition": {
    "variations": {
      "disabled": false,
      "enabled": true
    },
    "defaultRule": {
      "variation": "disabled"
    }
  },
  "cdn-mode": {
    "variations": {
      "standard": "cloudflare",
      "premium": "akamai",
      "multi": [
        "cloudflare",
        "akamai",
        "fastly"
      ]
    },
    "defaultRule": {
      "variation": "standard"
    }
  },
  "reporting-dashboard": {
    "variations": {
      "disabled": "none",
      "beta": "limited",
      "full": "full-access"
    },
    "defaultRule": {
      "variation": "beta"
    }
  }
}